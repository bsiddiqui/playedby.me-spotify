<div class="span12">
	<div class="search" id="spotify">
		<h1>Search for a Party</h1><br>
		<%= form_tag({:action => 'search'}, {:method => "get"}) do %>
			<%= label_tag :q, "Enter Playlist Code:" %>
			<%= text_field_tag(:q) %>
			<%= submit_tag("Find Playlist", :name => "submit") %>
		<% end %>
	</div>
</div>
